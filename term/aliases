alias net="speedtest-cli --secure --simple --bytes"
alias pfr="pfctl -sr"
alias pfs="pfctl -ss"
alias rm="rm -i"
alias s="dwmswallow $WINDOWID;"
alias speedtest="speedtest-cli --secure"
